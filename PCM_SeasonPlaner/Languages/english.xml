            <Horizontal Text="Horizontal"/>
            <Vertical Text="Vertical"/>
         </GroupOrientation>
      </TabView>
      <Messages>
         <NoDataLoaded Text="You need to open a plan or start a new one."/>
         <LoadingWaiting Text="Please wait until the data has been loaded."/>
