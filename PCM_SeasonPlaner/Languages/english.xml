<?xml version="1.0" encoding="utf-8" ?>
<PCM_SeasonPlaner>
   
   <MainWindow Text="PCM Season Planner">
      <ApplicationMenu>
         <PlanerNew Text="New plan"/>
         <DBExport Text="Export to cdb"/>
         <PlanerOpen Text="Open plan"/>
         <PlanerSave Text="Save plan"/>
         <LanguageSelection Text="Languages" ToolTip="Change the language of the application"/>
         <Options Text="Options" ToolTip="General options">
            <NotifySelection Text="Notify if too many cyclists"/>
            <SelectionPlaceholder Text="Prevent filling of cyclists"/>
         </Options>
         <Exit Text="Exit"/>
      </ApplicationMenu>
      <TabStart Text="Start">
