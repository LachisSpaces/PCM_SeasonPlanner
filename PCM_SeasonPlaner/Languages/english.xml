         <SecurityCheckUnsavedData Text="Do you want to save before you close the plan?"/>
         <FileNameInvalid Text="Invalid file name. Please remove all spaces from the name."/>
      </Messages>
   </MainWindow>

   <Page_StartPage Text="Main"></Page_StartPage>
   
