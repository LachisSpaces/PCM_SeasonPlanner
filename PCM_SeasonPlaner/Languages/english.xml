            <StartPage Text="Start page"/>
            <MyTeam Text="My team"/>
            <SeasonPlaner Text="Season planner"/>
            <SponsorScenario Text="Sponsor Scenario"/>
            <TeamOverview Text="Team overview"/>
            <CyclistOverview Text="Cyclist overview"/>
